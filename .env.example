# Cogitator Environment Variables
# Copy to .env and adjust as needed

# Redis (for RedisAdapter)
REDIS_URL=redis://localhost:6379

# Postgres (for PostgresAdapter)
POSTGRES_URL=postgres://cogitator:cogitator@localhost:5432/cogitator

# Ollama (for OllamaBackend and OllamaEmbeddingService)
OLLAMA_URL=http://localhost:11434

# OpenAI (optional, for OpenAIBackend and OpenAIEmbeddingService)
OPENAI_API_KEY=sk-your-key-here

# Anthropic (optional, for AnthropicBackend)
ANTHROPIC_API_KEY=sk-ant-your-key-here
