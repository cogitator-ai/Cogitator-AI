{
  "name": "@cogitator/workflows",
  "version": "0.1.0",
  "description": "DAG-based workflow engine for Cogitator agents",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
