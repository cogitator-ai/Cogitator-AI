{
  "name": "@cogitator-ai/core",
  "version": "0.18.2",
  "description": "Core runtime for Cogitator AI agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@cogitator-ai/memory": "workspace:*",
    "@cogitator-ai/models": "workspace:*",
    "@cogitator-ai/types": "workspace:*",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.10.9",
    "nanoid": "^5.0.4",
    "openai": "^6.22.0",
    "zod": "^4.3.6"
  },
  "optionalDependencies": {
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.991.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@cogitator-ai/sandbox": "workspace:*",
    "better-sqlite3": "^11.0.0",
    "langfuse": "^3.0.0",
    "pg": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-bedrock-runtime": {
      "optional": true
    },
    "@cogitator-ai/sandbox": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "langfuse": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cogitator-ai/Cogitator-AI.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT"
}
