{
  "name": "@cogitator-ai/rag",
  "version": "0.1.1",
  "description": "Retrieval Augmented Generation for Cogitator AI agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cogitator-ai/types": "workspace:*",
    "nanoid": "^5.0.4",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@cogitator-ai/core": "workspace:*",
    "@cogitator-ai/memory": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@cogitator-ai/core": {
      "optional": true
    },
    "@cogitator-ai/memory": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "cheerio": "^1.0.0",
    "papaparse": "^5.5.0",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@cogitator-ai/core": "workspace:*",
    "@cogitator-ai/memory": "workspace:*",
    "@types/papaparse": "^5.3.15",
    "@types/pdf-parse": "^1.1.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cogitator-ai/Cogitator-AI.git",
    "directory": "packages/rag"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT"
}
