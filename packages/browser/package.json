{
  "name": "@cogitator-ai/browser",
  "version": "0.1.0",
  "description": "Browser automation tools for Cogitator AI agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cogitator-ai/types": "workspace:*",
    "playwright": "^1.50.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@cogitator-ai/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cogitator-ai/Cogitator-AI.git",
    "directory": "packages/browser"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT"
}
